{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "pip install -r requirements.txt",
    "watchPatterns": ["**/*.py", "requirements.txt", "railway.json"]
  },
  "deploy": {
    "startCommand": "python arbitrage_main.py",
    "restartPolicyType": "ALWAYS",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300,
    "cron": null
  }
}
